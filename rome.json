{
	"$schema": "./node_modules/rome/configuration_schema.json",
	"organizeImports": {
		"enabled": false
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		},
		"ignore": [
			"build/*",
			"dist/*",
			"node_modules/*",
			"vendor/*",
			".nyc_output",
			"coverage",
			"_tools/*",
			".tap"
		]
	},
	"formatter": {
		"enabled": true,
		"ignore": [
			"build/*",
			"dist/*",
			"node_modules/*",
			"vendor/*",
			".nyc_output",
			"coverage",
			"_tools/*",
			".tap"
		]
	}
}
